{
    "name": "easy-vsx",
    "version": "1.6.0",
    "description": "Develop VS Code Extension Easier.",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hyy215/easy-vsx.git"
    },
    "engines": {
        "vscode": "^1.46.0"
    },
    "files": [
        "lib/**/*"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:step-input-demo"
    ],
    "main": "./lib/index.js",
    "contributes": {
        "commands": [
            {
                "command": "step-input-demo",
                "title": "Step Input Demo"
            }
        ]
    },
    "scripts": {
        "prepare": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "pretty": "prettier --write \"src/**/*.ts\""
    },
    "author": "hyy",
    "license": "MIT",
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.46.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "prettier": "^2.7.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    }
}
